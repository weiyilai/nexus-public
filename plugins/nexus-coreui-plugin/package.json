{
  "name": "@sonatype/nexus-coreui-plugin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all --parallel build-dev build-prod",
    "build-all": "npm-run-all format build-dev build-prod",
    "build-dev": "rspack --config rspack.dev.js",
    "build-prod": "rspack --config rspack.prod.js",
    "check-format": "biome format --changed --no-errors-on-unmatched",
    "format": "biome format --changed --write --no-errors-on-unmatched",
    "test": "npm-run-all check-format  test-coverage",
    "test-coverage": "jest --config src/frontend/jest.config.js --coverage",
    "test-watch": "jest --config src/frontend/jest.config.js --watch",
    "test-watch-debug": "node --inspect-brk node_modules/.bin/jest --config src/frontend/jest.config.js --runInBand --watchAll",
    "watch": "rspack --config rspack.dev.js  --watch"
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "@biomejs/biome": "1.9.4",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.13.1",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@nivo/core": "^0.85.0",
    "@nivo/line": "^0.85.0",
    "@sonatype/react-shared-components": "14.0.3",
    "@swc/jest": "^0.2.37",
    "@uirouter/react": "1.0.7",
    "@xstate/react": "3.2.2",
    "axios": "1.6.4",
    "classnames": "2.3.1",
    "luxon": "1.28.1",
    "ramda": "0.28.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swagger-ui-react": "4.19.1",
    "xstate": "4.38.3"
  },
  "devDependencies": {
    "@rspack/cli": "1.2.7",
    "@rspack/core": "1.2.7",
    "@sonatype/nexus-ui-plugin": "workspace:*",
    "@swc/core": "1.11.7",
    "@swc/helpers": "0.5.15",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "12.8.3",
    "@uirouter/visualizer": "7.2.1",
    "acorn": "7.0.0",
    "copy-modules-webpack-plugin": "2.2.1",
    "css-loader": "6.6.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-junit": "15.0.0",
    "jest-when": "3.1.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "sass": "1.38.0",
    "sass-loader": "12.6.0",
    "webpack-merge": "6.0.1"
  },
  "jest-junit": {
    "outputDirectory": "target/jest-reports",
    "outputName": "TEST-react-jest.xml",
    "reportTestSuiteErrors": true
  }
}
